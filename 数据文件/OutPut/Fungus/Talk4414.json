    {
      "ItemID": 285,
      "Name": "对话",
      "Description": "对话",
      "Position": "(-2633.821,-1235.965)",
      "Commands": [
        {
          "ItemID": 338,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "事毕，你径直走到入口处，范阁主仍在此处等候。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 337,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "fanjie",
          "StoryText": "呵呵呵呵，阁下可还算有收获？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 339,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "范阁主说笑了，我若全无收获，怎舍得这么快便出来？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 346,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "想必阁主也是早已感悟过此物，知晓其中玄妙，才会特意在此等候吧。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 341,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "你察觉到他嘴角隐约露出了一丝会心的笑，但立刻就消失不见。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 342,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "fanjie",
          "StoryText": "阁下能如此说，在下便了然了。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 351,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "fanjie",
          "StoryText": "想必阁下踏过化神的门槛，只是迟或早而已，范某便提前恭贺阁下了。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 347,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "fanjie",
          "StoryText": "日后我阁若不慎落入危难之间，还望阁下能伸以援手。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 343,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "阁主过誉了，若有此事，在下必然不会坐视不理。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 345,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "fanjie",
          "StoryText": "哈哈哈哈哈，那范某便提前谢过阁下了。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 350,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "fanjie",
          "StoryText": "阁下刚有所顿悟，还是早点加以巩固为上。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 349,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "谢阁主关心，那在下便先行告退了。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 348,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "fanjie",
          "StoryText": "也好，在下便送你一程罢。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 344,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "你二人行至主阁前，相互作了一揖后，他便目送你御剑离开了。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 336,
          "CmdType": "Fungus.LoadScene"
        }
      ]
