    {
      "ItemID": 265,
      "Name": "归来",
      "Description": "归来",
      "Position": "(-2454.947,-1286.701)",
      "Commands": [
        {
          "ItemID": 267,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "敖灵道友，幸不辱命。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 268,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 819,
          "AvatarBindKey": "",
          "StoryText": "灵儿，我回来了。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 269,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "敖灵看着你们脸上难掩激动的神色。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 270,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 820,
          "AvatarBindKey": "",
          "StoryText": "兄长，我就知道你没事的。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 271,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 819,
          "AvatarBindKey": "",
          "StoryText": "灵儿，我被困的这段时间，辛苦你了。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 272,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 820,
          "AvatarBindKey": "",
          "StoryText": "不辛苦，这都是灵儿应该的。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 273,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "敖灵转向你。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 274,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 820,
          "AvatarBindKey": "",
          "StoryText": "这枚天道果还请道友收好。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 275,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 820,
          "AvatarBindKey": "",
          "StoryText": "道友，我就先助我兄长疗伤。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 277,
          "CmdType": "Fungus.AddItem"
        },
        {
          "ItemID": 278,
          "CmdType": "Fungus.AddHaoGanDu"
        },
        {
          "ItemID": 279,
          "CmdType": "Fungus.AddHaoGanDu"
        },
        {
          "ItemID": 282,
          "CmdType": "Fungus.SetTaskIndexFinish"
        },
        {
          "ItemID": 283,
          "CmdType": "Fungus.SetStaticValue"
        },
        {
          "ItemID": 284,
          "CmdType": "Fungus.SetStaticValue"
        },
        {
          "ItemID": 295,
