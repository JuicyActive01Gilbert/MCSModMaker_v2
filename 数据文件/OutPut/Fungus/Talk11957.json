    {
      "ItemID": 394,
      "Name": "台座",
      "Description": "台座",
      "Position": "(-2797.667,-1630.778)",
      "Commands": [
        {
          "ItemID": 404,
          "CmdType": "Fungus.SetVariable"
        },
        {
          "ItemID": 405,
          "CmdType": "Fungus.GetStaticValue"
        },
        {
          "ItemID": 406,
