        {
          "ItemID": 297,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 622,
          "AvatarBindKey": "",
          "StoryText": "宗主平时都在白帝楼闭关，俗事就由老仆来打理。{w}{w=0.5}",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 293,
          "CmdType": "Fungus.ElseIf",
          "Condition": "TempValue == 4"
