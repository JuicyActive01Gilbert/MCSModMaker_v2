    {
      "ItemID": 314,
      "Name": "关于报酬",
      "Description": "关于报酬",
      "Position": "(-2696.436,-1094.516)",
      "Commands": [
        {
          "ItemID": 324,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "Avatar",
          "StoryText": "说说吧，你能拿出什么让我感兴趣的东西？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 325,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1304,
          "AvatarBindKey": "avater2",
          "StoryText": "道友之前化神这个境界可有了解？",
