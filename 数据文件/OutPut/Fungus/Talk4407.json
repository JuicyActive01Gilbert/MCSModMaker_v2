        {
          "ItemID": 473,
          "CmdType": "YSGame.TianJiDaBi.CmdTianJiDaBiStart"
        },
        {
          "ItemID": 474,
          "CmdType": "YSGame.TianJiDaBi.CmdTianJiDaBiGuanZhanSim"
