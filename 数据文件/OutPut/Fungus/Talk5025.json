        },
        {
          "ItemID": 720,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "杜老二",
          "StoryText": "哈哈哈哈，看来你的小弟也不怎么顶用啊？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 721,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
