        {
          "ItemID": 375,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "幽荧1",
          "StoryText": "保护我的是姐姐，需要保护的是妹妹，就这么叫下来啦。",
