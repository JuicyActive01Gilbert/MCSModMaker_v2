        },
        {
          "ItemID": 374,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "三妹不是很强嘛？为什么还需要保护？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 376,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "幽荧1",
          "StoryText": "有天赋只是一方面，但其实她对人的认识却是最少的，就像小孩子一样，要哄才行。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 377,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "原来如此…",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
