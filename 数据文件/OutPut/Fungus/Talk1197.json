        {
          "ItemID": 341,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 622,
          "AvatarBindKey": "",
          "StoryText": "阁下可是前来探访家主的？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 343,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "不知家主恢复得如何了？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 342,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 622,
          "AvatarBindKey": "",
          "StoryText": "多亏了阁下的灵药，家主现已无碍了。前些日子家主有些好转时曾提醒过老仆，若阁下到访便带您前往白帝楼。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 347,
          "CmdType": "Fungus.SetSTalk"
        },
        {
          "ItemID": 346,
          "CmdType": "Fungus.LoadScene"
        },
        {
          "ItemID": 351,
          "CmdType": "Fungus.Else"
        },
        {
          "ItemID": 353,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 622,
          "AvatarBindKey": "",
          "StoryText": "家主还在休息中，请阁下月后再来吧。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 355,
          "CmdType": "Fungus.GetNowTime"
        },
        {
          "ItemID": 356,
