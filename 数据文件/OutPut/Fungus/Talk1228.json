      "Commands": [
        {
          "ItemID": 330,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "闻言，那位修士略有不悦地眯起了眼睛。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 332,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 616,
          "AvatarBindKey": "",
          "StoryText": "阁下成心与我作对？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 331,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "哼，说者无意，听者有心罢了。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 334,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 616,
          "AvatarBindKey": "",
          "StoryText": "我看阁下倒不像无意之言，手下见真章如何？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 333,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "正有此意！",
          "Description": "",
