    {
      "ItemID": 380,
      "Name": "拜师青蛇",
      "Description": "拜师青蛇",
      "Position": "(-2065.79,-1616.896)",
      "Commands": [
        {
          "ItemID": 385,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "师尊在上，请受徒儿一拜。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 386,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 204,
          "AvatarBindKey": "",
          "StoryText": "哈哈哈，不错。你入我门下，本座必会悉心教导你的。\n之前墨景山所用的是《五毒噬心决》，乃是本门地阶毒法，你觉得如何。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 387,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "墨师兄所使的毒功，甚是霸道，对阵之时只能以灵力强行压制。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 388,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 204,
          "AvatarBindKey": "",
          "StoryText": "倒是有点眼光，拿着吧。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 389,
          "CmdType": "Fungus.AddItem"
        },
        {
          "ItemID": 390,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "多谢，师尊。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 391,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 204,
          "AvatarBindKey": "",
          "StoryText": "安心修炼，最好在猎魔试炼之中，为宗门争光。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 392,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "弟子谨遵教诲。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 393,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 204,
          "AvatarBindKey": "",
          "StoryText": "若有要事，可来后山寻我。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
