        {
          "ItemID": 447,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "嗯，知道了。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 448,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "Avatar",
          "StoryText": "那在下便静候前辈佳音了。",
