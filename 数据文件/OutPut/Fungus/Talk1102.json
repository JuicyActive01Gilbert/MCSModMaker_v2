        },
        {
          "ItemID": 359,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "你感受到，一股温和的神识之力注入你的识海。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 357,
