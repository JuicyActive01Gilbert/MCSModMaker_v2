      "Commands": [
        {
          "ItemID": 467,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "白袍修士",
          "StoryText": "前辈可还记得数百年前宁王府之事？",
