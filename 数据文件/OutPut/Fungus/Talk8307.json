    {
      "ItemID": 243,
      "Name": "剧情",
      "Description": "剧情",
      "Position": "(-2431.862,-1045.189)",
      "Commands": [
        {
          "ItemID": 291,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPC2",
          "StoryText": "抬眼望去，半空中漂浮着一人。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 292,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPC1",
          "StoryText": "这位前辈是谁啊？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 293,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPC2",
          "StoryText": "孤陋寡闻了吧，这位是绝焰堂大长老，钟焰长老。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 294,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPC1",
          "StoryText": "绝焰堂的前辈？这可不多见呢。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 296,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPC2",
          "StoryText": "确实，我上次也是在执行麟庙相关的任务之时侥幸遇到，钟长老还指点了我几句。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 295,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPC1",
          "StoryText": "噤声，掌门来了。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 297,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPChenghuo",
          "StoryText": "钟师兄，怎么今天这么有兴致啊。可是修为又有精进？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 298,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "炎师弟，你莫要取笑我了，哪有那么容易？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 299,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "今天，是家师归来之日。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 300,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPChenghuo",
          "StoryText": "麟尊大人回来了？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 301,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPChenghuo",
          "StoryText": "不对，他老人家不是一直在闭关准备....？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 302,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "（一脸无奈的看着对方）",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 303,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPChenghuo",
          "StoryText": "啊~~懂了。那近些年辛苦师兄了。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 304,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPC2",
          "StoryText": "远处一团火焰，飞速靠近。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 305,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyi",
          "StoryText": "徒儿为师回来啦~",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 306,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "弟子钟焰，恭迎师尊归来。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 307,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPC2",
          "StoryText": "众人：“弟子，恭迎麟尊。”",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 308,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyi",
          "StoryText": "嗯~都散了吧。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 309,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyi",
          "StoryText": "哈哈哈，为师可是有不少收获呢。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 310,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyan",
          "StoryText": "那徒弟只能恭喜您啦。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 311,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPCzhongyi",
          "StoryText": "这个....哈哈哈，先回家，先回家~",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 312,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "NPC2",
          "StoryText": "说罢，一行人并做一道遁光直入麟庙。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 1069,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "一道传音自麟庙中传出。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 1070,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 442,
          "AvatarBindKey": "",
          "StoryText": "{FirstName}{shidi}，家师想要见见你，不知可否方便？",
