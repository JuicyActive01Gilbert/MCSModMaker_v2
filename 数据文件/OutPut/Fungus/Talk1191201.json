      "Position": "(-2985.218,-1227.703)",
      "Commands": [
        {
          "ItemID": 323,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 935,
          "AvatarBindKey": "",
          "StoryText": "莫要怪我，我也是奉命行事，受死吧。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 324,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 931,
          "AvatarBindKey": "",
          "StoryText": "奉命?哪家的啊？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 325,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 935,
          "AvatarBindKey": "",
          "StoryText": "白帝楼？？！！",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 326,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 931,
          "AvatarBindKey": "",
          "StoryText": "你是自己伏法还是要吃点苦头？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 327,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 935,
          "AvatarBindKey": "",
          "StoryText": "抓我？你还不够格...",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 328,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "只见李天面色痛苦，但周身泛起血雾，向远处疾驰而去。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 329,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 931,
          "AvatarBindKey": "",
          "StoryText": "哼，不自量力。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 330,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "刚刚冲出客栈的李天便被一道剑光击落，迅速有人现身，将其制服。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 331,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "",
          "StoryText": "这白帝楼做事倒是滴水不漏，这周围已有人部下剑阵，{xiaozi}多学学啊。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 332,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "...\n",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 333,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 931,
          "AvatarBindKey": "",
          "StoryText": "血遁术，嗨，这些宵小之辈也就这点手段了。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 334,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 931,
          "AvatarBindKey": "",
          "StoryText": "小友，这次你有些冒失了，这厮已经加入古神教，颇有几分神通。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 335,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 931,
          "AvatarBindKey": "",
          "StoryText": "多亏了客栈老板通知我，此番才能及时赶到。这是你的奖励。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 336,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 931,
          "AvatarBindKey": "",
          "StoryText": "我手头还有要事处理，此事你自行回白帝楼找当值执事汇报吧。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 337,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "多谢前辈相救。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
