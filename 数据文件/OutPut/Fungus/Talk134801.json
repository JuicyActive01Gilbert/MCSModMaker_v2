    {
      "ItemID": 267,
      "Name": "药田",
      "Description": "药田",
      "Position": "(-2442.052,-1307.529)",
      "Commands": [
        {
          "ItemID": 268,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "你眼前是一片灵药田，不同于你之前所见的药田，其中有丝丝龙脉之力散发而出。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 269,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "",
          "StoryText": "这灵药园应该是龙族培养药材的地方，更容易催生出龙皇果之类的药材。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 270,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "嗯，龙族相较于我们人族，堪称天之宠儿。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 271,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "你神识扫过药田，并没有什么发现。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 272,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "可惜了，此地荒废太久了。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        }
      ]
    }
