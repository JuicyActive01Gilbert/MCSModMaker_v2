    {
      "ItemID": 1214,
      "Name": "询问",
      "Description": "询问",
      "Position": "(-2386.019,-1123.483)",
      "Commands": [
        {
          "ItemID": 1226,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "星铃儿嘛，不是我女儿哦。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 1227,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "Avatar",
          "StoryText": "{size=24}她就是宫主...{/size}",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 1228,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "Avatar",
          "StoryText": "啊？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 1229,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "我就是{$Xing}哦，嘻嘻。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 1231,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "...",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 1232,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "星铃儿就是你，而你就是星宫宫主？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 1233,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "是的呢。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 1234,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "...",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 1235,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "你当了多少年星宫宫主了？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 1236,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "星宫宫主啊，应该有一千多年了吧。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 1237,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "...",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 1238,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "所以，我们第一次相遇时，你根本不需要我救...",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 1239,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "是啊，当时我不是说了嘛，如果认真起来他可不是我对手。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 1240,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "...",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 1241,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "第二次相遇时，你一个人就能把那邪教剿灭，结果还让我对上教主。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 1242,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "那次可是你主动要加入的哦，而且我也是知道他肯定不是你对手，才让你去的呢。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 1243,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "那三百年前在林家那次，你明明就是宫主，为何还引来冲虚散人，这可不是我主动的了吧？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 1244,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "那不是你和心儿散布谣言害我废了好多心力平息在先嘛，我只是想扳回一局。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 1245,
          "CmdType": "Fungus.Say",
