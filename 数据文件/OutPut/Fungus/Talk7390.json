        },
        {
          "ItemID": 1181,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "你赶到林沐心和神秘修士的战场时，却看到宫主正背对着你与林沐心交流着什么。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 1182,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "Avatar",
          "StoryText": "{FirstName}{$NickName}！",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 1190,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "林沐心越过宫主看到你到来，很是活泼地向你打着招呼，好像...只是出来游山玩水而已...",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 1191,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "（唔，难道是我想太多，{$Lin}刚才的惊呼并不是因为出现了意外么？）",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 1192,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "{FirstName}道友，此番多谢了，要不是你，我和心儿只怕是要受困于冲虚散人与古神教之手。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 1193,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "Avatar",
          "StoryText": "{size=28}不要叫我心儿...{/size}",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 1194,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "xingning",
          "StoryText": "之前指点你修炼的时候，可都是这么叫的呢，心儿。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 1195,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "Avatar",
          "StoryText": "谁...谁知道...",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
