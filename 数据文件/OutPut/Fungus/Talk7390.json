        {
          "ItemID": 1324,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "Avatar",
          "StoryText": "我...们带宫主回天星城吧，至少...带她回家。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
