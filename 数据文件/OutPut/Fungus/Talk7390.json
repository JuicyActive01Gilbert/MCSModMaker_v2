          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 1212,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "Avatar",
          "StoryText": "我...我...我分辨不出来啦！",
