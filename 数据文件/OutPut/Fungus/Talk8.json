          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 201,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 605,
          "AvatarBindKey": "",
          "StoryText": "好说，好说。",
