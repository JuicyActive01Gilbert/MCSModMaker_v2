    {
      "ItemID": 513,
      "Name": "最终",
      "Description": "最终",
      "Position": "(-2066.07,-1306.056)",
      "Commands": [
        {
          "ItemID": 526,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "只见空间一阵波动，高空中撕开了一个极其不规整的空间裂口，一个黑影从中倒飞而出。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 527,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "这…难道说她失败了？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 528,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "神识扫去，只见她身形异常狼狈，身体肉眼可见的有几处缺口，其中阴气正向外逸散。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 529,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "幽荧3",
          "StoryText": "（邪魅地）怎么可能？？？ ",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 530,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "幽荧3",
          "StoryText": "为什么会找不到？？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 531,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "幽荧3",
          "StoryText": "我明明感应到了才对？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 532,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "幽荧3",
          "StoryText": "哪里出问题了？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 533,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "她眼神呆滞，不停地自言自语着。你有些担心她的状况，便飞了过去。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 537,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "荧儿，没事吧？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 534,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "听了你的话后，她猛然一怔，随即又莫名暴怒起来，不由分说地向你攻来。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 535,
          "CmdType": "Fungus.SetSTalk"
        },
        {
          "ItemID": 536,
          "CmdType": "Fungus.StartFight"
        }
      ]
