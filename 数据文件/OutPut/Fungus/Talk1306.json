    {
      "ItemID": 145,
      "Name": "南崖城",
      "Description": "南崖城",
      "Position": "(-2371.92,-1114.753)",
      "Commands": [
        {
          "ItemID": 551,
          "CmdType": "Fungus.GetStaticValue"
        },
        {
