        },
        {
          "ItemID": 435,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "黑心盗取宗门至宝嗜血珠，天邪子奉命追击，尔等酌情配合。",
          "Description": "",
          "AvatarIDSetType": "Normal"
