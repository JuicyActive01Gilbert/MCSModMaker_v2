    {
      "ItemID": 284,
      "Name": "化劫丹",
      "Description": "化劫丹",
      "Position": "(-2626.402,-1305.411)",
      "Commands": [
        {
          "ItemID": 308,
          "CmdType": "Fungus.GetStaticValue"
