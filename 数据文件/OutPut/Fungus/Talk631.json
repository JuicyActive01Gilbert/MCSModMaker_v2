      "Position": "(-1932.106,-1255.646)",
      "Commands": [
        {
          "ItemID": 513,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "一道人影突然出现在掌门身后，察觉到异样的掌门猛然转过身来。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 515,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "掌门id",
          "StoryText": "？！是谁？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 517,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "在看清那人样貌之后，掌门又变得恭敬起来。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 518,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "掌门id",
          "StoryText": "恭迎师祖，不知师祖出关所为何事？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 516,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "化神老祖",
          "StoryText": "你待会便知道了。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 519,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "而这一切，自然也被你看在眼里。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 522,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "你不想浪费时间，身形一闪，便出现在大殿上。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 514,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "师兄。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 521,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "化神老祖",
          "StoryText": "{shidi}，恭喜。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 523,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "见此情形，掌门瞬间便明白了事情的原委，便识趣地退至一旁，不再说话。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 525,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "化神老祖",
          "StoryText": "按照惯例，此等喜事应当昭告天下，让全宁州的能人志士前来道贺。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 524,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "多谢师兄，但这凡俗之礼就不必了。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 528,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "想必师兄也心知肚明，此次前来，我主要是想向掌门询问些事宜。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 527,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "化神老祖",
          "StoryText": "好说，{shidi}想了解什么，我定然倾囊相授。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
