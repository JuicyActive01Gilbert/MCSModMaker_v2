    {
      "ItemID": 400,
      "Name": "关于天道？",
      "Description": "关于天道？",
      "Position": "(-1385.776,-1124.564)",
      "Commands": [
        {
          "ItemID": 450,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "讲述者",
          "StoryText": "天道便是诸条大道的尽头。所谓大道归一，所归之处即为天道。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 454,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "讲述者",
          "StoryText": "万物运行之法则，功法神通之根源，也皆为天道。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 455,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 2,
          "AvatarBindKey": "讲述者",
          "StoryText": "就算仅窥得一丝天道，所领悟的仙术便能轻易超越世间任何神通。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 546,
