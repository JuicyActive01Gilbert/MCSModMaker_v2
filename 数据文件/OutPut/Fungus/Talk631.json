        {
          "ItemID": 471,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "你看出掌门此番用意，只是释然一笑。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 472,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "这是自然。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 475,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "倘若宗门有难，我必然不会坐视不理。",
          "Description": "",
          "AvatarIDSetType": "Normal"
