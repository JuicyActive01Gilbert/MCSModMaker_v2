        {
          "ItemID": 310,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "你随着灵气指引逐渐深入小巷，一缕令人生恶的气息在空气中弥散。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 316,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
