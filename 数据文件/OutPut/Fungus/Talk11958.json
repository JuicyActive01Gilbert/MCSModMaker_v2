      "Description": "New Block",
      "Position": "(-3050.667,-1249.278)",
      "Commands": [
        {
          "ItemID": 326,
          "CmdType": "Fungus.GetStaticValue"
        },
        {
          "ItemID": 327,
          "CmdType": "Fungus.If",
          "Condition": "TempValue < 4"
