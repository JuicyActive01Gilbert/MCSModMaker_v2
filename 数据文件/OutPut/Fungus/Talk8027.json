        },
        {
          "ItemID": 589,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "Avatar",
          "StoryText": "你时而妙语生机口生莲花，时而掐指施法此方灵气因你而动。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 590,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "Avatar",
          "StoryText": "台下弟子或若有所思，或皱眉参悟。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 567,
          "CmdType": "Fungus.FadeScreen"
