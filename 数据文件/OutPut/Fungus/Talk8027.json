        },
        {
          "ItemID": 706,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "或是因为厚积薄发，短短数日她竟提升了一个小境界。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 707,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "Avatar",
          "StoryText": "呼~多谢长老授业解惑之恩。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
        },
        {
          "ItemID": 708,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "嗯，无需挂心，我还有别的事先走了。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 709,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "Avatar",
          "StoryText": "恭送长老。",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
