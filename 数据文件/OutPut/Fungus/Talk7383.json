        {
          "ItemID": 433,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 620,
          "AvatarBindKey": "NpcId",
          "StoryText": "公孙府向来只产上中下三品盈手露，不知{FirstName}道友从何听说极品一说？",
          "Description": "",
          "AvatarIDSetType": "FungusVariable"
