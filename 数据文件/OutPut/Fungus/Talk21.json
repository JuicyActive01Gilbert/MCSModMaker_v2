        }
      ]
    },
    {
      "ItemID": 294,
      "Name": "愿闻其详",
      "Description": "愿闻其详",
      "Position": "(-2864.662,-1067.884)",
      "Commands": [
        {
          "ItemID": 295,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 914,
          "AvatarBindKey": "",
          "StoryText": "我天机阁主要分为主阁，天衍阁和拍卖行。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 298,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 914,
          "AvatarBindKey": "",
          "StoryText": "主阁，乃是阁主之所在，如无要事还请道友不要靠近。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 297,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 914,
          "AvatarBindKey": "",
          "StoryText": "天衍阁是我天机阁售卖情报玉简之所道友要事有兴趣探探秘境，不凡去看看。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 296,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 914,
          "AvatarBindKey": "",
          "StoryText": "拍卖行自是举行拍卖会的地方，10年一届，金丹期修士便可参加。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 299,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 914,
          "AvatarBindKey": "",
          "StoryText": "道友自己转转吧，我就不打扰了。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        }
      ]
