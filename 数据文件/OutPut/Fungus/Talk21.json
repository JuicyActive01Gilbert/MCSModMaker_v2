      "Commands": [
        {
          "ItemID": 291,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 0,
          "AvatarBindKey": "",
          "StoryText": "刚刚进入大门，远处一名修士向你走来。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 288,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 914,
          "AvatarBindKey": "",
          "StoryText": "阁下是第一次来天机阁吧。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 289,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 1,
          "AvatarBindKey": "",
          "StoryText": "正是，阁下是？",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 290,
          "CmdType": "Fungus.Say",
          "AvatarIDInt": 914,
          "AvatarBindKey": "",
          "StoryText": "在下天机阁接引执事，就由在下向你介绍一下吧。",
          "Description": "",
          "AvatarIDSetType": "Normal"
        },
        {
          "ItemID": 292,
          "CmdType": "Fungus.Menu",
          "Text": "愿闻其详",
          "TargetBlockID": "this:294(愿闻其详)"
        },
        {
          "ItemID": 293,
          "CmdType": "Fungus.Menu",
          "Text": "我自己转转就好了。",
